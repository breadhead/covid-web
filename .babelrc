{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@app": "./",
          "@front": "./src"
        }
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        "next/babel",
        "@zeit/next-typescript/babel"
      ]
    },
    "production": {
      "presets": [
        "next/babel",
        "@zeit/next-typescript/babel"
      ]
    },
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"]
    }
  }
}
